POSTGRES_USER="my_user"
POSTGRES_PASSWORD="my_password"
POSTGRES_DB="my_db"
POSTGRESS_PORT=5432
DATABASE_URL=postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:${POSTGRESS_PORT}/${POSTGRES_DB}